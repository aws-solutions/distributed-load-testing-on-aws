{
  "name": "distributed-load-testing-on-aws-ui",
  "version": "3.3.0",
  "private": true,
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions"
  },
  "description": "Distributed Load Testing on AWS UI",
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false react-scripts build",
    "clean": "rm -rf node_modules package-lock.json",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test --watchAll=false --coverage --silent"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@aws-amplify/pubsub": "^6.0.16",
    "@aws-amplify/ui-react": "^6.1.14",
    "aws-amplify": "^6.4.3",
    "aws-sdk": "^2.1457.0",
    "bootstrap": "^5.1.0",
    "bootstrap-icons": "^1.8.1",
    "brace": "^0.11.1",
    "chart.js": "^4.0.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-js-cron": "^5.0.1",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^9.0.0",
    "cron-parser": "^4.9.0",
    "solution-utils": "file:../solution-utils"
  },
  "overrides": {
    "nth-check": "2.0.1",
    "@aws-sdk/client-s3": "3.414.0",
    "@aws-sdk/client-sts": "3.414.0",
    "resolve-url-loader": "5.0.0",
    "axios": "^1.6.0",
    "react-refresh": "0.14.0",
    "fast-xml-parser": "4.4.1"
  },
  "readme": "./README.md",
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "eslint-config-react-app": "^7.0.1",
    "react-scripts": "^5.0.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(react-js-cron|axios)/)"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ]
  }
}
